<h1>Users#show</h1>

<br>
<%= @user.email %>
<br>
<%= @user.name %>
<br>
<% if logged_in? %>
  Logged in as: <%= current_user.name %>
  <br>
  <h3>Events</h3>
  <br>
  <%@user.created_events.each do |event| %>
  <%= event.description %>
  <br>
  <hr>
  <%end%>
<%else%>
  <br>
<%= link_to "Login", login_path %>
<%end%>